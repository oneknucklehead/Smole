{
    "version": 2,
    "builds": [
      {
        "src": "./frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "./backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "http://localhost:5000/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "./frontend/build/$1"
      }
    ]
  }